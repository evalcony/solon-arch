# hutool-db
# 方便地进行数据库操作

url = jdbc:mysql://localhost:3306/cur_use?useUnicode=true&characterEncoding=utf8&autoReconnect=true&rewriteBatchedStatements=true
user = root
pass = 123456

## 可选配置
# 是否在日志中显示执行的SQL
showSql = true
# 是否格式化显示的SQL
formatSql = false
# 是否显示SQL参数
showParams = true
# 打印SQL的日志等级，默认debug，可以是info、warn、error
sqlLevel = debug